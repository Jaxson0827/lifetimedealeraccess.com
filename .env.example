# Copy this file to .env.local for local development:
#   cp .env.example .env.local
#
# Never commit real secrets. Use your hosting provider''s env settings in production.

## Stripe
# Required: Stripe secret key (test or live)
STRIPE_SECRET_KEY=

# Optional: Use a Stripe Price ID instead of hard-coded $100
STRIPE_SETUP_DEPOSIT_PRICE_ID=

## Email (Resend)
# Required: Resend API key
RESEND_API_KEY=

# Required: Verified sender in Resend (can include display name)
# Example: Lifetime Auto <no-reply@lifetimedealeraccess.com>
EMAIL_FROM=

# Optional: Override destination inbox for contact messages.
# Defaults to Kevin.lifetimeauto@gmail.com if not set.
CONTACT_TO_EMAIL=

## GoHighLevel / LeadConnector
# Required: Webhook trigger URL from your GoHighLevel workflow
GOHIGHLEVEL_WEBHOOK_URL=

# Optional: Pipeline automation (opportunities)
GOHIGHLEVEL_PIPELINE_ID=
GOHIGHLEVEL_STAGE_ID=
